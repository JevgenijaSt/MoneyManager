<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.misel.moneymanager">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".DeleteSpendingActivity"></activity>
        <activity android:name=".DeleteItemActivity" />
        <activity android:name=".DiagramsActivity" />
        <activity
            android:name=".ShowExpenditureActivity"
            android:label="@string/exp_show"
            android:parentActivityName=".ExpenditureActivity" />
        <activity android:name=".InfoActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".IncomeActivity"
            android:label="@string/income_name"
            android:parentActivityName=".MainActivity" />

        <meta-data
            android:name="android.support.PARENT_ACTIVITY"
            android:value=".MainActivity" />

        <activity
            android:name=".IncomeShowDataActivity"
            android:label="@string/income_show"
            android:parentActivityName=".IncomeActivity" />
        <activity
            android:name=".ExpenditureActivity"
            android:label="@string/exp_name"
            android:parentActivityName=".MainActivity" />

        <meta-data
            android:name="android.support.PARENT_ACTIVITY"
            android:value=".MainActivity" />
    </application>

</manifest>